Proceso nombreCapitalyPais_4
	
	Definir capital Como Caracter;
	Definir pais Como Caracter;
	
	Escribir "Ingresa el nombre de una ciudad capital: ";
	Leer capital;
	
	Escribir "Ingresa el nombre del país correspondiente: ";
	Leer pais;
	
	Limpiar Pantalla;
	
	Escribir "La ciudad ", capital, " es la capital del país ", pais;
	
FinProceso
